<h1>Listing order_items</h1>

<table>
  <tr>
    <th>Order</th>
    <th>Title</th>
    <th>Price cents</th>
    <th>Image url</th>
    <th>Seller zip code</th>
    <th>Seller country</th>
  </tr>

<% for order_item in @order_items %>
  <tr>
    <td><%=h order_item.order_id %></td>
    <td><%=h order_item.title %></td>
    <td><%=h order_item.price_cents %></td>
    <td><%=h order_item.image_url %></td>
    <td><%=h order_item.seller_zip_code %></td>
    <td><%=h order_item.seller_country %></td>
    <td><%= link_to 'Show', order_item %></td>
    <td><%= link_to 'Edit', edit_order_item_path(order_item) %></td>
    <td><%= link_to 'Destroy', order_item, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New order_item', new_order_item_path %>
